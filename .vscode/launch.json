{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "launch",
        "name": "Launch Program",
        "skipFiles": ["<node_internals>/**"],
        "program": "${workspaceFolder}\\app.js"
      }
    ],

    "inputs": [
    {
      "id": "pickCmd",
      "description": "input command",
      "type": "pickString",
      "options": ["List All", "List Sam N", "Close"],
      "default": "Close"
    }
    ]
  }
  